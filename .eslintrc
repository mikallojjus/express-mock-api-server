{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"
  ],
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/resolver": {
      "typescript": {}
    }
  },
  "env": {
    "node": true,
    "es6": true
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-empty": 0,
    "@typescript-eslint/naming-convention": 0,
    "react/prop-types": 0,
    "@typescript-eslint/ban-ts-comment": 0,
    "@typescript-eslint/explicit-function-return-type": 0,
    "@typescript-eslint/no-explicit-any": 1,
    "@typescript-eslint/member-ordering": 1,
    "@typescript-eslint/no-unused-vars": 2,
    "@typescript-eslint/member-delimiter-style": [
      "error",
      {
        "multiline": {
          "delimiter": "none",
          "requireLast": false
        },
        "singleline": {
          "delimiter": "comma",
          "requireLast": false
        }
      }
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "import/default": 0,
    "import/namespace": 0,
    "import/no-named-as-default-member": 0,
    "import/named": 0,
    "import/order": [
      "error",
      {
        "groups": [
          [
            "internal",
            "external",
            "builtin"
          ],
          "index",
          "parent",
          "sibling"
        ],
        "newlines-between": "always"
      }
    ],
    "comma-dangle": 0,
    "prefer-arrow-callback": "error",
    "linebreak-style": 0,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "endOfLine": "auto"
      }
    ]
  }
}
